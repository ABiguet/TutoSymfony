{# filepath: templates/contact/form.html.twig #}

{% extends 'base.html.twig' %}

{% block body %}
    <h1>Nous contacter</h1>
    {{ form_start(form) }}
    <div class="row">
        <div class="col-sm">{{ form_row(form.nom) }}</div>
        <div class="col-sm"> {{ form_row(form.email) }}</div>
        <div class="col-sm">{{ form_row(form.service) }}</div>
    </div>
    {{ form_end(form) }}
{% endblock %}